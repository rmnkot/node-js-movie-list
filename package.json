{
  "name": "node-js-movie-list",
  "version": "1.0.0",
  "description": "Application for creating your own lists of movies and series, based on using a third party service http://www.omdbapi.com.",
  "main": "app.js",
  "scripts": {
    "start": "nodemon ./src/index.ts",
    "build": "tsc --build",
    "debug": "node --inspect-brk --require ts-node/register ./src/index.ts",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "db:seed:undo:all": "npx sequelize-cli db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rmnkot/node-js-movie-list.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rmnkot/node-js-movie-list/issues"
  },
  "homepage": "https://github.com/rmnkot/node-js-movie-list#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.11.6",
    "@types/uuid": "^8.3.1",
    "@types/validator": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "sequelize-cli": "^6.3.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.12.0-alpha.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  }
}
